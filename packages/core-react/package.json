{
  "name": "@ferlab-ui/core-react",
  "version": "0.1.0",
  "description": "Core components for scientific research data portals",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/Ferlab-Ste-Justine/ferlab-ui.git"
  },
  "module": "lib/es/index.js",
  "esnext": "lib/esnext/index.js",
  "typings": "lib/es/index.d.ts",
  "exports": {
    "./": "./lib/esnext/",
    "./es/": "./lib/es/"
  },
  "scripts": {
    "build:watch": "tsc -w -p ./src --declaration -t esnext --outDir ./",
    "build": "npm-run-all build:es build:esnext",
    "build:es": "tsc -p ./src --declaration --outDir lib/es",
    "build:esnext": "tsc -p ./src --declaration -t esnext --outDir ./",
    "build:scss": "sass ./src:./lib/esnext",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Ferlab - Centre de recherche du CHU Ste-Justine",
  "license": "SEE LICENSE IN LICENSE",
  "devDependencies": {
    "@types/antd": "^1.0.0",
    "@types/react": "^16.9.51",
    "@types/react-router-dom": "^5.1.6",
    "core-js": "^3.6.5",
    "npm-run-all": "^4.1.5",
    "sass": "^1.26.12",
    "typescript": "^4.0.3",
    "webpack-cli": "^3.3.12",
    "@typescript-eslint/eslint-plugin": "^4.10.0",
    "@typescript-eslint/parser": "^4.10.0",
    "eslint": "^7.15.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-prettier": "^3.3.0",
    "prettier": "^2.2.1"
  },
  "peerDependencies": {
    "antd": "^4.2.0",
    "react": "^16.0.0",
    "@ferlab-ui/core-style": "^0.0.9"
  },
  "dependencies": {
    "@types/classnames": "^2.2.11",
    "classnames": "^2.2.6"
  },
  "bugs": {
    "url": "https://github.com/Ferlab-Ste-Justine/ferlab-ui/issues"
  },
  "homepage": "https://github.com/Ferlab-Ste-Justine/ferlab-ui#readme",
  "main": "index.js"
}
