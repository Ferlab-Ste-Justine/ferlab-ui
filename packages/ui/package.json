{
    "name": "@ferlab/ui",
    "version": "10.24.2",
    "description": "Core components for scientific research data portals",
    "publishConfig": {
        "access": "public"
    },
    "bin": {
        "ferlabui-script": "themes/bin/style-script.js",
        "ferlabui-less2css": "themes/bin/less2css.js"
    },
    "repository": {
        "type": "git",
        "url": "git+ssh://git@github.com/Ferlab-Ste-Justine/ferlab-ui.git"
    },
    "module": "core/",
    "sideEffects": false,
    "typings": "index.d.ts",
    "files": [
        "core/**/*",
        "themes/**/*"
    ],
    "scripts": {
        "fix-linter": "eslint --fix --rule 'no-console: 2'",
        "build": "npm-run-all build:es build:esnext && tsc && tscp",
        "build:core": "tsc && tscp",
        "build:es": "tsc --declaration --sourceMap --outDir lib/es",
        "build:esnext": "tsc --declaration --declarationMap",
        "lint": "eslint $(git diff-index --name-only --diff-filter=d HEAD ../../packages/ui | grep -E '\\.ts$|\\.tsx$|\\.js$|\\.jsx$' | xargs | sed 's/packages\\/ui\\///g')",
        "lint:all": "eslint",
        "lint:fix": "npm run lint -- --fix",
        "start": "tsc -w --declaration & tscp -w",
        "test": "jest ./src",
        "test:watch": "jest --watch",
        "test:github": "jest --coverage --watchAll=false --no-watchma  --coverageReporters=\"text-summary\"",
        "coverage": "jest --watchAll=false --coverage --coverageReporters=\"json-summary\"",
        "coverage:changed": "jest --coverage --watchAll=false --onlyChanged --coverageReporters=\"json-summary\"",
        "compile:colors": "./themes/bin/style-script.js colors"
    },
    "author": "Ferlab - Centre de recherche du CHU Ste-Justine",
    "license": "SEE LICENSE IN LICENSE",
    "devDependencies": {
        "@testing-library/dom": "^10.1.0",
        "@testing-library/jest-dom": "^6.4.5",
        "@testing-library/react": "^16.0.0",
        "@types/classnames": "^2.2.11",
        "@types/d3": "^7.4.3",
        "@types/jest": "^29.2.3",
        "@types/lodash": "^4.14.168",
        "@types/md5": "^2.3.2",
        "@types/react": "^18.3.1",
        "@types/react-grid-layout": "^1.3.5",
        "@types/react-highlight-words": "^0.16.4",
        "@types/uuid": "^8.3.0",
        "@typescript-eslint/eslint-plugin": "^4.10.0",
        "@typescript-eslint/parser": "^4.10.0",
        "core-js": "^3.6.5",
        "date-fns": "^2.29.3",
        "eslint": "^7.15.0",
        "eslint-config-prettier": "^7.0.0",
        "eslint-plugin-prefer-arrow": "^1.2.3",
        "eslint-plugin-prettier": "^3.3.0",
        "eslint-plugin-react": "^7.22.0",
        "eslint-plugin-simple-import-sort": "^7.0.0",
        "eslint-plugin-sort-destructure-keys": "^1.3.5",
        "eslint-plugin-sort-keys-fix": "^1.1.1",
        "jest-environment-jsdom": "^29.3.1",
        "npm-run-all": "^4.1.5",
        "prettier": "^2.2.1",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "sass": "^1.26.12",
        "sass-loader": "^10.5.0",
        "ts-jest": "^29.1.2",
        "ts-loader": "^9.5.1",
        "ts-node": "^10.9.1",
        "tsconfig-paths-webpack-plugin": "^4.1.0",
        "typescript": "^4.9.5",
        "typescript-cp": "^0.1.7",
        "webpack": "^5.89.0"
    },
    "peerDependencies": {
        "@sentry/react": "^8.23.0",
        "antd": "^4.24.16",
        "date-fns": "^2.29.3",
        "rc-tree-select": "^5.4.0",
        "react": "^18.3.1",
        "react-dom": "^18.3.1"
    },
    "overrides": {
        "@ant-design/icons": {
            "react": "$react"
        },
        "@testing-library/react": {
            "react": "$react",
            "react-dom": "$react-dom"
        }
    },
    "dependencies": {
        "@ant-design/icons": "^4.8.3",
        "@ant-design/icons-svg": "^4.4.2",
        "@dnd-kit/core": "^4.0.3",
        "@dnd-kit/sortable": "^5.1.0",
        "@dnd-kit/utilities": "^3.2.0",
        "@nivo/bar": "^0.87.0",
        "@nivo/core": "^0.87.0",
        "@nivo/pie": "^0.87.0",
        "@nivo/tooltip": "^0.87.0",
        "@react-spring/web": "^9.7.4",
        "antd-img-crop": "^4.2.4",
        "classnames": "^2.2.6",
        "command-line-args": "^5.1.1",
        "cross-spawn": "^7.0.6",
        "d3": "^7.9.0",
        "d3-svg-to-png": "^0.3.1",
        "history": "^4.9.0",
        "less": "^4.1.0",
        "md5": "^2.3.0",
        "npm": "^10.8.2",
        "query-string": "^7.0.1",
        "react-grid-layout": "^1.5.0",
        "react-highlight-words": "^0.18.0",
        "react-icons": "^4.2.0",
        "react-sizeme": "^3.0.2",
        "react-zoom-pan-pinch": "^3.6.1",
        "simplebar-react": "^2.4.3",
        "uuid": "^8.3.2"
    },
    "bugs": {
        "url": "https://github.com/Ferlab-Ste-Justine/ferlab-ui/issues"
    },
    "homepage": "https://github.com/Ferlab-Ste-Justine/ferlab-ui#readme"
}
